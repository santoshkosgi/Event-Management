<%= form_for @user,:url => { :controller => "events",:action => "create1" }, :html => { :multipart => true } do |f| %>
  <div class="clearfix">
      <%= f.label :name %>
      <div class="input">
        <%= f.text_field :name,:value =>$omniauth['user_info']['name'] %>
      </div>
  </div>
  <div class="clearfix">
      <%= f.label :email %>
      <div class="input">
        <%= f.text_field :email%>
      </div>
  </div>

  <div class="actions">
    <%= f.submit "Submit", :class=>"btn primary" %>
  </div>

<% end %>
